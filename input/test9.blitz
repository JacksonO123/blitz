struct [T] shape {
  area: u32;
  value: T;
}

struct [G] rectangle : shape<G> {
  width: u32;
  height: u32;
  pub nothing: G;
}

struct [T: rectangle<char>] square {
  pub inner: T;
  size: u32;

   pub static fn [G] create(size: G, inner: T) square<T> {
     return square<T> {
       inner,
       size
     };
   }
}

let r: rectangle<char> = rectangle<char> {
  area = 5 * 10,
  // failes with wrong value
  // value = true,
  value = 'b',
  width = 10,
  height = 5,
  nothing = 'o'
};

// let n = 10.0;
let n = 10;
let s: square<rectangle<char>> = square.create(n, r);

let a: char = s.inner.nothing;

